	<changeSet author="bmandel" id="1529608689312-1">
	    <createTable tableName="CONFIG_FIELD" schemaName="ALERT">
            <column autoIncrement="true" name="ID" type="BIGINT(19)">
                <constraints primaryKey="true" primaryKeyName="CONSTRAINT_5"/>
            </column>
            <column name="FIELD_NAME" type="VARCHAR(255)"/>
            <column name="FIELD_TYPE" type="VARCHAR(255)"/>
            <column name="FIELD_VALUE" type="VARCHAR(255)"/>
            <column name="FIELD_SECURE" type="BOOLEAN(1)"/>
            <column name="FIELD_REQUIRED" type="BOOLEAN(1)"/>
            <column name="CONFIG_ID" type="BIGINT(19)"/>
            <column name="CONFIG_TYPE" type="VARCHAR(255)"/>
        </createTable>
	</changeSet>
	<changeSet author="bmandel" id="1529608689312-2">
	    <insert tableName="CONFIG_FIELD" schemaName="ALERT">
            <column name="FIELD_NAME" value="CHANNEL_CONFIG"/>
            <column name="CONFIG_TYPE" value="COMMON_DISTRIBUTION"/>
        </insert>
	</changeSet>
